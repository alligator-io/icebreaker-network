{
  "name": "icebreaker-network",
  "version": "0.8.3",
  "description": "a network library for pull-streams",
  "main": "index.js",
  "scripts": {
    "build": "npm run browserify && npm run dist",
    "test": "node scripts/test",
    "test-local": "tape test/*.js test/node/*.js",
    "test-zuul": "zuul -- test/browser/connect.js",
    "test-browser": "zuul --open --local 8087 -- test/browser/connect.js",
    "browserify": "browserify ./browser.js --ignore ws --ignore os --ignore net --ignore dns --exclude icebreaker  -s network  -o dist/icebreaker-network.js",
    "dist": "uglifyjs dist/icebreaker-network.js -o dist/icebreaker-network.min.js"
  },
  "author": "Markus Wunderlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alligator-io/icebreaker-network/issues"
  },
  "homepage": "https://github.com/alligator-io/icebreaker-network",
  "repository": {
    "type": "git",
    "url": "https://github.com/alligator-io/icebreaker-network.git"
  },
  "optionalDependencies": {
    "utf-8-validate": "^5.0.2",
    "bufferutil": "^4.0.1",
    "utp-native": "^2.1.3"
  },
  "dependencies": {
    "bs58": "^4.0.1",
    "chloride": "^2.2.10",
    "datagram-stream": "^1.1.1",
    "icebreaker": "^2.0.3",
    "ip-address": "^5.8.9",
    "lodash.defaults": "^4.2.0",
    "lodash.pick": "^4.4.0",
    "pull-defer": "^0.2.3",
    "pull-paramap": "^1.2.2",
    "pull-ws": "^3.3.1",
    "secret-handshake": "^1.1.14"
   },
  "devDependencies": {
    "browserify": "^16.1.0",
    "pull-goodbye": "^0.0.2",
    "pull-serializer": "^0.3.2",
    "pull-stream": "^3.6.9",
    "pull-abortable": "^4.1.1",
    "tape": "^4.9.1",
    "uglify-js": "^3.4.9",
    "zuul": "^3.12.0"
  }
}
